#include<iostream>
#include<vector>
using namespace std;
int main()
{
    vector<long long int> vec;
    long long int i,n,x=1,cnt=0;
    cin >>n;

    for(i=0;i<64;i++)
    {
        x =x*2;
        vec.push_back(x);
    }
    for(int i:vec)
    {
        if(n==i)
        {
            cnt++;
            break;
        }
    }
    if(cnt>0)
    {
        cout <<"N is power of 2";

    }else cout << "N is not a power of 2";
}
